				<toolbar mold="panel" align="center">
					<button label="确定" width="65px" id="okButton"></button>
					<button label="取消" width="65px" id="cancelButton">
					</button>
				</toolbar>
			</panelchildren>
		</panel>
	</bandpopup>
</zk>
