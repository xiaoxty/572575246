<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 2.0//EN" "http://getahead.org/dwr/dwr20.dtd">

<dwr>
	<allow>
		<convert converter="bean" match="java.lang.StackTraceElement" />
		<convert converter="exception" match="java.lang.Throwable" />
		<convert converter="bean" match="cn.ffcs.uom.common.vo.*" />
		<convert converter="bean" match="cn.ffcs.uom.information.vo.*" />

		<create creator="spring" javascript="orgTreeAction">
			<param name="beanName" value="orgTreeAction" />
		</create>
		<create creator="spring" javascript="informationAction">
			<param name="beanName" value="informationAction" />
		</create>

	</allow>
</dwr>