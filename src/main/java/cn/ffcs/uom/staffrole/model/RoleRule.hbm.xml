<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.ffcs.uom.staffrole.model.RoleRule" table="ROLE_RULE">
		<comment>角色规则</comment>
		<id name="roleRuleId" type="java.lang.Long">
			<column name="ROLE_RULE_ID" precision="22" scale="0" />
		</id>
		<property name="ruleType" type="java.lang.String">
			<column name="RULE_TYPE" length="10">
			</column>
		</property>
		<property name="role" type="java.lang.Long">
			<column name="ROLE" length="20">
			</column>
		</property>
		<property name="roleRela" type="java.lang.Long">
			<column name="ROLE_RELA" length="20">
			</column>
		</property>
		<property name="statusCd" type="java.lang.String">
			<column name="STATUS_CD" length="10">
			</column>
		</property>
		<property name="message" type="java.lang.String">
			<column name="MESSAGE" length="50">
			</column>
		</property>
	</class>
</hibernate-mapping>
