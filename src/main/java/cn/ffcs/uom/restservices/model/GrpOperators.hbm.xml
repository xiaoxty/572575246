<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.ffcs.uom.restservices.model.GrpOperators" table="GRP_OPERATORS">
		<comment>经营主体标识</comment>
		<id name="grpOperatorsId" type="java.lang.Long">
			<column name="GRP_OPERATORS_ID" precision="12" scale="0" />
			<generator class="sequence">
				<param name="sequence">SEQ_GRP_OPERATORS_ID</param>
			</generator>
		</id>
		<property name="operatorsNbr" type="java.lang.String">
			<column name="OPERATORS_NBR" length="30">
				<comment>经营主体编码</comment>
			</column>
		</property>
		<property name="operatorsName" type="java.lang.String">
			<column name="OPERATORS_NAME" length="200">
				<comment>经营主体名称</comment>
			</column>
		</property>
		<property name="operatorsTypeCd" type="java.lang.String">
			<column name="OPERATORS_TYPE_CD" length="6">
				<comment>经营主体类型</comment>
			</column>
		</property>
		<property name="certType" type="java.lang.String">
			<column name="CERT_TYPE" length="6">
				<comment>经营主体证件类型</comment>
			</column>
		</property>
		<property name="certNbr" type="java.lang.String">
			<column name="CERT_NBR" length="60">
				<comment>经营主体证件号码</comment>
			</column>
		</property>
		<property name="operatorsSname" type="java.lang.String">
			<column name="OPERATORS_SNAME" length="30">
				<comment>经营主体简称</comment>
			</column>
		</property>
		<property name="legalRepr" type="java.lang.String">
			<column name="LEGAL_REPR" length="64">
				<comment>法人代表</comment>
			</column>
		</property>
		<property name="adderss" type="java.lang.String">
			<column name="ADDRESS" length="250">
				<comment>联系地址</comment>
			</column>
		</property>
		<property name="telephone" type="java.lang.String">
			<column name="TELEPHONE" length="64">
				<comment>联系电话</comment>
			</column>
		</property>
		<property name="contact" type="java.lang.String">
			<column name="CONTACT" length="64">
				<comment>联系人</comment>
			</column>
		</property>
		<property name="email" type="java.lang.String">
			<column name="EMAIL" length="64">
				<comment>邮箱</comment>
			</column>
		</property>
		<property name="orgId" type="java.lang.Long">
			<column name="ORG_ID" length="12">
				<comment>组织结构标识</comment>
			</column>
		</property>
		<property name="operatorsAreaGrade" type="java.lang.String">
			<column name="OPERATORS_AREA_GRADE" length="6">
				<comment>经营主体级别</comment>
			</column>
		</property>
		<property name="parentOperNbr" type="java.lang.String">
			<column name="PARENT_OPER_NBR" length="30">
				<comment>上级经营主体编码</comment>
			</column>
		</property>
		<property name="commonRegionId" type="java.lang.String">
			<column name="COMMON_REGION_ID" length="30">
				<comment>公用管理区域标识</comment>
			</column>
		</property>
		<property name="description" type="java.lang.String">
			<column name="DESCRIPTION" length="2000">
				<comment>经营主体描述</comment>
			</column>
		</property>
		<property name="action" type="java.lang.String">
			<column name="ACTION" length="6">
				<comment>处理动作</comment>
			</column>
		</property>
		<property name="effDate" type="java.util.Date">
			<column name="EFF_DATE" length="7">
				<comment>生效时间</comment>
			</column>
		</property>
		<property name="expDate" type="java.util.Date">
			<column name="EXP_DATE" length="7">
				<comment>失效时间</comment>
			</column>
		</property>
		<property name="statusCd" type="java.lang.String">
			<column name="STATUS_CD" length="6">
				<comment>状态</comment>
			</column>
		</property>
		<property name="statusDate" type="java.util.Date">
			<column name="STATUS_DATE" length="7">
				<comment>状态时间</comment>
			</column>
		</property>
		<property name="createDate" type="java.util.Date">
			<column name="CREATE_DATE" length="7">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="createStaff" type="java.lang.Long">
			<column name="CREATE_STAFF" length="12">
				<comment>创建人</comment>
			</column>
		</property>
		<property name="updateDate" type="java.util.Date">
			<column name="UPDATE_DATE" length="7">
				<comment>最后修改时间</comment>
			</column>
		</property>
		<property name="updateStaff" type="java.lang.Long">
			<column name="UPDATE_STAFF" length="12">
				<comment>修改人</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
