<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.ffcs.uom.restservices.model.GrpStaff" table="GRP_STAFF">
		<comment>员工标识</comment>
		<id name="grpStaffId" type="java.lang.Long">
			<column name="GRP_STAFF_ID" precision="12" scale="0" />
			<generator class="sequence">
				<param name="sequence">SEQ_GRP_STAFF_ID</param>
			</generator>
		</id>
		<property name="salesCode" type="java.lang.String">
			<column name="SALES_CODE" length="100">
				<comment>员工编码</comment>
			</column>
		</property>
		<property name="staffCode" type="java.lang.String">
			<column name="STAFF_CODE" length="30">
				<comment>员工工号</comment>
			</column>
		</property>
		<property name="orgId" type="java.lang.Long">
			<column name="ORG_ID" length="12">
				<comment>组织结构标识</comment>
			</column>
		</property>
		<property name="staffName" type="java.lang.String">
			<column name="STAFF_NAME" length="250">
				<comment>员工姓名</comment>
			</column>
		</property>
		<property name="certType" type="java.lang.String">
			<column name="CERT_TYPE" length="6">
				<comment>证件类型</comment>
			</column>
		</property>
		<property name="certNumber" type="java.lang.String">
			<column name="CERT_NUMBER" length="30">
				<comment>员工身份证号</comment>
			</column>
		</property>
		<property name="mobilePhone" type="java.lang.String">
			<column name="MOBILE_PHONE" length="32">
				<comment>员工联系手机</comment>
			</column>
		</property>
		<property name="email" type="java.lang.String">
			<column name="E_MAIL" length="100">
				<comment>员工联系邮箱</comment>
			</column>
		</property>
		<property name="commonRegionId" type="java.lang.String">
			<column name="COMMON_REGION_ID" length="30">
				<comment>公用管理区域标识</comment>
			</column>
		</property>
		<property name="staffDesc" type="java.lang.String">
			<column name="STAFF_DESC" length="250">
				<comment>员工描述</comment>
			</column>
		</property>
		<property name="action" type="java.lang.String">
			<column name="ACTION" length="6">
				<comment>处理动作</comment>
			</column>
		</property>
		<property name="effDate" type="java.util.Date">
			<column name="EFF_DATE" length="7">
				<comment>生效时间</comment>
			</column>
		</property>
		<property name="expDate" type="java.util.Date">
			<column name="EXP_DATE" length="7">
				<comment>失效时间</comment>
			</column>
		</property>
		<property name="statusCd" type="java.lang.String">
			<column name="STATUS_CD" length="6">
				<comment>状态</comment>
			</column>
		</property>
		<property name="statusDate" type="java.util.Date">
			<column name="STATUS_DATE" length="7">
				<comment>状态时间</comment>
			</column>
		</property>
		<property name="createDate" type="java.util.Date">
			<column name="CREATE_DATE" length="7">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="createStaff" type="java.lang.Long">
			<column name="CREATE_STAFF" length="12">
				<comment>创建人</comment>
			</column>
		</property>
		<property name="updateDate" type="java.util.Date">
			<column name="UPDATE_DATE" length="7">
				<comment>最后修改时间</comment>
			</column>
		</property>
		<property name="updateStaff" type="java.lang.Long">
			<column name="UPDATE_STAFF" length="12">
				<comment>修改人</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
