<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.ffcs.uom.roleauth.model.StaffAuthority" table="RBAC_STAFF_AUTHORITY">
		<comment>角色标识</comment>
		<id name="authorityId" type="java.lang.Long">
			<column name="AUTHORITY_ID" precision="12" scale="0" />
			<generator class="sequence">
				<param name="sequence">SEQ_RBAC_STAFF_AUTH_ID</param>
			</generator>
		</id>
		<property name="authorityName" type="java.lang.String">
			<column name="AUTHORITY_NAME" length="120">
				<comment>权限名称</comment>
			</column>
		</property>
		<property name="authorityParentId" type="java.lang.Long">
			<column name="AUTHORITY_PARENT_ID" length="12">
				<comment>父权限标识</comment>
			</column>
		</property>
		<property name="isParent" type="java.lang.String">
			<column name="IS_PARENT" length="1">
				<comment>是否是父节点</comment>
			</column>
		</property>
		<property name="effDate" type="java.util.Date">
			<column name="EFF_DATE" length="7">
				<comment>生效时间</comment>
			</column>
		</property>
		<property name="expDate" type="java.util.Date">
			<column name="EXP_DATE" length="7">
				<comment>失效时间</comment>
			</column>
		</property>
		<property name="statusCd" type="java.lang.String">
			<column name="STATUS_CD" length="6">
				<comment>状态</comment>
			</column>
		</property>
		<property name="statusDate" type="java.util.Date">
			<column name="STATUS_DATE" length="7">
				<comment>状态时间</comment>
			</column>
		</property>
		<property name="createDate" type="java.util.Date">
			<column name="CREATE_DATE" length="7">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="createStaff" type="java.lang.Long">
			<column name="CREATE_STAFF" length="12">
				<comment>创建人</comment>
			</column>
		</property>
		<property name="updateDate" type="java.util.Date">
			<column name="UPDATE_DATE" length="7">
				<comment>最后修改时间</comment>
			</column>
		</property>
		<property name="updateStaff" type="java.lang.Long">
			<column name="UPDATE_STAFF" length="12">
				<comment>修改人</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
