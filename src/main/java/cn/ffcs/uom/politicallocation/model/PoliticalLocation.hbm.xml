<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.ffcs.uom.politicallocation.model.PoliticalLocation"
		table="POLITICAL_LOCATION">
		<comment>行政管理区域</comment>
		<id name="locationId" type="java.lang.Long">
			<column name="LOCATION_ID" precision="12" scale="0" />
			<generator class="sequence">
				<param name="sequence">SEQ_POLITICAL_LOCATION_ID</param>
			</generator>
		</id>
		<property name="locationCode" type="java.lang.String">
			<column name="LOCATION_CODE" length="30">
				<comment>行政区域编码</comment>
			</column>
		</property>
		<property name="locationName" type="java.lang.String">
			<column name="LOCATION_NAME" length="250">
				<comment>行政区域名称</comment>
			</column>
		</property>
		<property name="locationDesc" type="java.lang.String">
			<column name="LOCATION_DESC" length="250">
				<comment>行政区域描述</comment>
			</column>
		</property>
		<property name="locationType" type="java.lang.String">
			<column name="LOCATION_TYPE" length="3">
				<comment>行政区域类型</comment>
			</column>
		</property>
		<property name="locationAbbr" type="java.lang.String">
			<column name="LOCATION_ABBR" length="30">
				<comment>行政区域简拼</comment>
			</column>
		</property>
		<property name="upLocationId" type="java.lang.Long">
			<column name="UP_LOCATION_ID" length="12">
				<comment>上级行政区域标识</comment>
			</column>
		</property>
		<property name="effDate" type="java.util.Date">
			<column name="EFF_DATE" length="7">
				<comment>生效时间</comment>
			</column>
		</property>
		<property name="expDate" type="java.util.Date">
			<column name="EXP_DATE" length="7">
				<comment>失效时间</comment>
			</column>
		</property>
		<property name="statusCd" type="java.lang.String">
			<column name="STATUS_CD" length="6">
				<comment>状态</comment>
			</column>
		</property>
		<property name="statusDate" type="java.util.Date">
			<column name="STATUS_DATE" length="7">
				<comment>状态时间</comment>
			</column>
		</property>
		<property name="createStaff" type="java.lang.Long">
			<column name="CREATE_STAFF" length="12">
				<comment>创建人</comment>
			</column>
		</property>
		<property name="createDate" type="java.util.Date">
			<column name="CREATE_DATE" length="7">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="updateStaff" type="java.lang.Long">
			<column name="UPDATE_STAFF" length="12">
				<comment>修改人</comment>
			</column>
		</property>
		<property name="updateDate" type="java.util.Date">
			<column name="UPDATE_DATE" length="7">
				<comment>修改时间</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
