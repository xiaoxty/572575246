<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.ffcs.uom.party.model.Party" table="party">
		<comment>参与人标识</comment>
		<id name="partyId" type="java.lang.Long">
			<column name="PARTY_ID" precision="12" scale="0" />
			<generator class="sequence">
				<param name="sequence">SEQ_PARTY_ID</param>
			</generator>
		</id>
		<property name="partyName" type="java.lang.String">
			<column name="PARTY_NAME" length="250">
				<comment>参与人名称</comment>
			</column>
		</property>
		<property name="partyNameFirst" type="java.lang.String">
			<column name="PARTY_NAME_FIRST" length="250">
				<comment>参与人曾名称</comment>
			</column>
		</property>
		<property name="partyAbbrname" type="java.lang.String">
			<column name="PARTY_ABBRNAME" length="60">
				<comment>参与人简拼</comment>
			</column>
		</property>
		<property name="englishName" type="java.lang.String">
			<column name="ENGLISH_NAME" length="250">
				<comment>参与人英文名</comment>
			</column>
		</property>
		<property name="partyType" type="java.lang.String">
			<column name="PARTY_TYPE" length="6">
				<comment>参与人类型</comment>
			</column>
		</property>
		<property name="effDate" type="java.util.Date">
			<column name="EFF_DATE" length="7">
				<comment>生效时间</comment>
			</column>
		</property>
		<property name="expDate" type="java.util.Date">
			<column name="EXP_DATE" length="7">
				<comment>失效时间</comment>
			</column>
		</property>
		<property name="statusCd" type="java.lang.String">
			<column name="STATUS_CD" length="6">
				<comment>状态</comment>
			</column>
		</property>
		<property name="statusDate" type="java.util.Date">
			<column name="STATUS_DATE" length="7">
				<comment>状态时间</comment>
			</column>
		</property>
		<property name="createDate" type="java.util.Date">
			<column name="CREATE_DATE" length="7">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="createStaff" type="java.lang.Long">
			<column name="CREATE_STAFF" length="12">
				<comment>创建人</comment>
			</column>
		</property>
		<property name="updateDate" type="java.util.Date">
			<column name="UPDATE_DATE" length="7">
				<comment>最后修改时间</comment>
			</column>
		</property>
		<property name="updateStaff" type="java.lang.Long">
			<column name="UPDATE_STAFF" length="12">
				<comment>修改人</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
