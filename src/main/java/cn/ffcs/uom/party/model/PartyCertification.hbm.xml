<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.ffcs.uom.party.model.PartyCertification" table="PARTY_CERTIFICATION">
		<comment>参与人证件标识</comment>
		<id name="partyCertId" type="java.lang.Long">
			<column name="PARTY_CERT_ID" precision="12" scale="0" />
			<generator class="sequence">
				<param name="sequence">SEQ_PARTY_CERTIFICATION_ID</param>
			</generator>
		</id>
		<property name="partyId" type="java.lang.Long">
			<column name="PARTY_ID" length="12">
				<comment>参与人标识</comment>
			</column>
		</property>
		<property name="certName" type="java.lang.String">
			<column name="CERT_NAME" length="500">
				<comment>证件名</comment>
			</column>
		</property>
		<property name="isRealName" type="java.lang.String">
			<column name="IS_REAL_NAME" length="1">
				<comment>是否实名</comment>
			</column>
		</property>
		<property name="certType" type="java.lang.String">
			<column name="CERT_TYPE" length="6">
				<comment>证件类型</comment>
			</column>
		</property>
		<property name="certOrg" type="java.lang.String">
			<column name="CERT_ORG" length="250">
				<comment>发证机关</comment>
			</column>
		</property>
		<property name="certAddress" type="java.lang.String">
			<column name="CERT_ADDRESS" length="250">
				<comment>证件地址</comment>
			</column>
		</property>
		<property name="certNumber" type="java.lang.String">
			<column name="CERT_NUMBER" length="500">
				<comment>证件号码</comment>
			</column>
		</property>
		<property name="certSort" type="java.lang.String">
			<column name="CERT_SORT" length="6">
				<comment>证件种类</comment>
			</column>
		</property>
		<property name="effDate" type="java.util.Date">
			<column name="EFF_DATE" length="7">
				<comment>生效时间</comment>
			</column>
		</property>
		<property name="expDate" type="java.util.Date">
			<column name="EXP_DATE" length="7">
				<comment>失效时间</comment>
			</column>
		</property>
		<property name="statusCd" type="java.lang.String">
			<column name="STATUS_CD" length="6">
				<comment>状态</comment>
			</column>
		</property>
		<property name="statusDate" type="java.util.Date">
			<column name="STATUS_DATE" length="7">
				<comment>状态时间</comment>
			</column>
		</property>
		<property name="createDate" type="java.util.Date">
			<column name="CREATE_DATE" length="7">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="createStaff" type="java.lang.Long">
			<column name="CREATE_STAFF" length="12">
				<comment>创建人</comment>
			</column>
		</property>
		<property name="updateDate" type="java.util.Date">
			<column name="UPDATE_DATE" length="7">
				<comment>最后修改时间</comment>
			</column>
		</property>
		<property name="updateStaff" type="java.lang.Long">
			<column name="UPDATE_STAFF" length="12">
				<comment>修改人</comment>
			</column>
		</property>
		<property name="identityCardId" type="java.lang.Long">
			<column name="IDENTITY_CARD_ID" length="12">
				<comment>身份证类型ID</comment>
			</column>
		</property>
		<property name="reason" type="java.lang.String">
			<column name="REASON" length="200">
				<comment>原因</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
