<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="cn.ffcs.uom.common.treechooser.model.Node" table="ATTR_VALUE">
		<comment>属性值</comment>
		<id name="attrValueId" type="java.lang.Long">
			<column name="ATTR_VALUE_ID" precision="12" scale="0" />
			<generator class="assigned">
			</generator>
		</id>
		<property name="parentValueId" type="java.lang.Long">
			<column name="PARENT_VALUE_ID" length="12">
				<comment>上级属性ID</comment>
			</column>
		</property>
		<property name="attrId" type="java.lang.Long">
			<column name="ATTR_ID" length="12">
				<comment>属性ID</comment>
			</column>
		</property>
		<property name="attrValue" type="java.lang.String">
			<column name="ATTR_VALUE" length="1000">
				<comment>属性值</comment>
			</column>
		</property>
		<property name="attrValueName" type="java.lang.String">
			<column name="ATTR_VALUE_NAME" length="200">
				<comment>属性值名称</comment>
			</column>
		</property>
		<property name="attrDesc" type="java.lang.String">
			<column name="ATTR_DESC" length="1000">
				<comment>属性值描述</comment>
			</column>
		</property>
		<property name="effDate" type="java.util.Date">
			<column name="EFF_DATE" length="7">
				<comment>生效时间</comment>
			</column>
		</property>
		<property name="expDate" type="java.util.Date">
			<column name="EXP_DATE" length="7">
				<comment>失效时间</comment>
			</column>
		</property>
		<property name="statusCd" type="java.lang.String">
			<column name="STATUS_CD" length="6">
				<comment>状态</comment>
			</column>
		</property>
		<property name="statusDate" type="java.util.Date">
			<column name="STATUS_DATE" length="7">
				<comment>状态时间</comment>
			</column>
		</property>
		<property name="createDate" type="java.util.Date">
			<column name="CREATE_DATE" length="7">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="createStaff" type="java.lang.Long">
			<column name="CREATE_STAFF" length="12">
				<comment>创建人</comment>
			</column>
		</property>
		<property name="updateDate" type="java.util.Date">
			<column name="UPDATE_DATE" length="7">
				<comment>最后修改时间</comment>
			</column>
		</property>
		<property name="updateStaff" type="java.lang.Long">
			<column name="UPDATE_STAFF" length="12">
				<comment>修改人</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
