<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="cn.ffcs.uom.systemconfig.model.AttrExtValue" table="ATTR_EXT_VALUE">
		<comment>扩展属性值ID</comment>
		<id name="attrExtValueId" type="java.lang.Long">
			<column name="ATTR_EXT_VALUE_ID" precision="12" scale="0" />
			<generator class="sequence">
				<param name="sequence">SEQ_ATTR_EXT_VALUE_ID</param>
			</generator>
		</id>
		<property name="attrValueId" type="java.lang.Long">
			<column name="ATTR_VALUE_ID" length="12">
				<comment>属性值ID</comment>
			</column>
		</property>
		<property name="attrExtValue" type="java.lang.String">
			<column name="ATTR_EXT_VALUE" length="1000">
				<comment>属性值</comment>
			</column>
		</property>
		<property name="attrExtValueName" type="java.lang.String">
			<column name="ATTR_EXT_VALUE_NAME" length="200">
				<comment>属性值名称</comment>
			</column>
		</property>
		<property name="attrExtDesc" type="java.lang.String">
			<column name="ATTR_EXT_DESC" length="1000">
				<comment>属性值描述</comment>
			</column>
		</property>
		<property name="effDate" type="java.util.Date">
			<column name="EFF_DATE" length="7">
				<comment>生效时间</comment>
			</column>
		</property>
		<property name="expDate" type="java.util.Date">
			<column name="EXP_DATE" length="7">
				<comment>失效时间</comment>
			</column>
		</property>
		<property name="statusCd" type="java.lang.String">
			<column name="STATUS_CD" length="6">
				<comment>状态</comment>
			</column>
		</property>
		<property name="statusDate" type="java.util.Date">
			<column name="STATUS_DATE" length="7">
				<comment>状态时间</comment>
			</column>
		</property>
		<property name="createDate" type="java.util.Date">
			<column name="CREATE_DATE" length="7">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="createStaff" type="java.lang.Long">
			<column name="CREATE_STAFF" length="12">
				<comment>创建人</comment>
			</column>
		</property>
		<property name="updateDate" type="java.util.Date">
			<column name="UPDATE_DATE" length="7">
				<comment>最后修改时间</comment>
			</column>
		</property>
		<property name="updateStaff" type="java.lang.Long">
			<column name="UPDATE_STAFF" length="12">
				<comment>修改人</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
