<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.ffcs.uom.staff.model.StaffOrganizationTran"
		table="STAFF_ORGANIZATION_TRAN">
		<comment>员工组织业务关系标识</comment>
		<id name="staffOrgTranId" type="java.lang.Long">
			<column name="STAFF_ORG_TRAN_ID" precision="12" scale="0" />
			<generator class="sequence">
				<param name="sequence">seq_staff_organization_tran_id</param>
			</generator>
		</id>
		<property name="staffId" type="java.lang.Long">
			<column name="STAFF_ID" length="12">
				<comment>员工标识</comment>
			</column>
		</property>
		<property name="orgId" type="java.lang.Long">
			<column name="ORG_ID" length="12">
				<comment>组织标识</comment>
			</column>
		</property>
		<property name="orgTreeId" type="java.lang.Long">
			<column name="ORG_TREE_ID" length="12">
				<comment>组织树标识</comment>
			</column>
		</property>
		<property name="ralaCd" type="java.lang.String">
			<column name="RALA_CD" length="6">
				<comment>关联类型</comment>
			</column>
		</property>
		<property name="staffSort" type="java.lang.Long">
			<column name="STAFF_SORT" length="12">
				<comment>员工排序</comment>
			</column>
		</property>
		<property name="effDate" type="java.util.Date">
			<column name="EFF_DATE" length="7">
				<comment>生效时间</comment>
			</column>
		</property>
		<property name="expDate" type="java.util.Date">
			<column name="EXP_DATE" length="7">
				<comment>失效时间</comment>
			</column>
		</property>
		<property name="statusCd" type="java.lang.String">
			<column name="STATUS_CD" length="6">
				<comment>状态</comment>
			</column>
		</property>
		<property name="statusDate" type="java.util.Date">
			<column name="STATUS_DATE" length="7">
				<comment>状态时间</comment>
			</column>
		</property>
		<property name="createDate" type="java.util.Date">
			<column name="CREATE_DATE" length="7">
				<comment>创建时间</comment>
			</column>
		</property>
		<property name="createStaff" type="java.lang.Long">
			<column name="CREATE_STAFF" length="12">
				<comment>创建人</comment>
			</column>
		</property>
		<property name="updateDate" type="java.util.Date">
			<column name="UPDATE_DATE" length="7">
				<comment>最后修改时间</comment>
			</column>
		</property>
		<property name="updateStaff" type="java.lang.Long">
			<column name="UPDATE_STAFF" length="12">
				<comment>修改人</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>
